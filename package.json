{
  "name": "treetype-ts",
  "description": "treetype but ts and magic",
  "version": "1.1.7",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "ttgen": "dist/index.js"
  },
  "scripts": {
    "start": "node --no-warnings=ExperimentalWarning .",
    "build": "rimraf dist && tsc",
    "dev": "rimraf dist && tsc-watch --onSuccess \"pnpm run start\"",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redstonekasi/treetype-ts.git"
  },
  "bugs": "https://github.com/redstonekasi/treetype-ts/issues",
  "homepage": "https://github.com/redstonekasi/treetype-ts#readme",
  "author": "redstonekasi",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "rimraf": "^4.1.2",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cleye": "^1.3.1",
    "scule": "^1.0.0",
    "ts-morph": "^17.0.1"
  }
}
